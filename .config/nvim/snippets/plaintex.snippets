snippet dcl \documentclass{}
	\\documentclass{${1:class}} ${0}


snippet tit italic text
    \\textit{$1}$0

snippet tbf bold text
    \\textbf{$1}$0


snippet be \begin \end block
    \begin{$1}
        $0
    \end{$1}


snippet se \section{}
    \section{$1}$0

snippet sse \subsection{}
    \subsection{$1}$0

snippet ssse \subsubsection{}
    \subsubsection{$1}$0

snippet itemize itemize
    \begin{itemize}
        \item $0
    \end{itemize}



